          "style": {
            "height": "36px"
          },
          "value": "$model.login"
        },
        {
          "type": "input",
          "id": "password",
          "style": {
            "height": "36px"
          },
          "value": "$model.password"
        },
        {
          "type": "button",
          "id": "loginButton",
          "style": {
            "height": "36px"
          },
          "text": "Log in",
          "onClick": {
            "logic": {
              "variables": [
                {
                  "name": "response",
                  "type": "string",
                  "value": ""
                },
                {
                  "name": "error",
                  "type": "string",
                  "value": ""
                }
              ],
              "calls": [
                {
                  "_type": "SendRequest",
                  "urlExpr": "'http://localhost:44373/api/ApiAccount/Login'",
                  "method": "POST",
                  "dataExpr": "{ Email: $model.login, Password: $model.password }",
                  "variableName": "response",
                  "errorVariableName": "error",
                  "cacheResponse": false,
                  "withCredentials": true
                },
                {
                  "_type": "Switch",
                  "expr": "!!error",
                  "cases": [
                    {
                      "calls": [
                        {
                          "_type": "SetValue",
                          "leftExpr": "$global.authenticated",
                          "valueExpr": "true"
                        },
                        {
                          "_type": "NavigateToView",
                          "viewId": "IncidentsView"
                        }
                      ],
                      "valueExpr": "false"
                    }
                  ],
                  "otherwise": [
                    {
                      "_type": "ShowAlert",
                      "titleExpr": "Error",
                      "messageExpr": "Invalid email or password."
                    }
                  ]
                }
              ]
            }
          }
        }
      ]
    }
